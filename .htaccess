# Page not found
ErrorDocument 404 lvp-new/errors/404.html 

SetEnv PHP_VER 7
SetEnv REGISTER_GLOBALS 0

RewriteEngine On

# FOR HTTPS

#RewriteCond %{SERVER_PORT} 80
#RewriteRule ^(.*)$ https://wwww.lavaguepourpre.com/$1 [R,L]

RewriteRule home/?$ lvp-new/index.php [L]
RewriteRule actualites/?$ lvp-new/actualites.php [L]
RewriteCond %{HTTP_HOST} ^lavaguepourpre.com$
RewriteRule ^(.*) http://wwww.lavaguepourpre.com/$1 [QSA,L,R=301]


RewriteCond %{REQUEST_URI} ^/404/$
RewriteRule ^(.*)$ lvp-new/errors/404.html [L]
